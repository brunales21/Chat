<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="121.0" prefWidth="351.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.chatbmbis.AddViewController">
   <children>
      <HBox alignment="CENTER_LEFT" layoutX="-1.0" prefHeight="122.0" prefWidth="407.0">
         <children>
            <TextField fx:id="nickname" prefHeight="39.0" prefWidth="206.0" promptText="Nombre Usuario">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="addOrcreate" mnemonicParsing="false" onMouseClicked="#onClickAdd" prefHeight="43.0" prefWidth="117.0" text="Agregar usuario">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
